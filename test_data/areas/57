<h2>Templeton</h2>



<table class="data-table">
	<thead>
		<tr>
			<th>Location</th>
			<th width="15%">Recallable</th>
			<th width="25%">Who string</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td>(1630, 536) in Alyria</td>
			<td>Yes</td>
			<td>Towne</td>
		</tr>

				<tr>
			<td colspan="3">You see <em>the shimmering walls and rooftops of the towne of Templeton</em>.</td>
		</tr>
		</table>

<script type="text/javascript">
addLoadEvent(function() {
	vmap = new Vmap(document.getElementById('area-vmap'));
		vmap.setPlane(1);
	vmap.setCenterPos([1630, 536]);
	vmap.setZoom(4);
	vmap.setInteractivity(false);
	vmap.create();
	vmap.replacePOI({
		name: 'area',
		coordinates: [1630, 536],
		className: 'textpointer',
		html: "Templeton",
		loaded: function(e, vmap) { var $this = $(this); $this.css({marginLeft: -$this.outerWidth() / 2, marginTop: -$this.outerHeight() - 10}); }
	});


	map = new Map(document.getElementById('area-smap'));
	map.setZone(57, 0);
	map.statusCallback = function(s) { document.getElementById('mapstatus').innerHTML = s; }
});

function vmaptoggle() {
	document.getElementById('vmaptab').style.display = 'block';
	document.getElementById('smaptab').style.display = 'none';
}
function smaptoggle() {
	document.getElementById('vmaptab').style.display = 'none';
	document.getElementById('smaptab').style.display = 'block';
}

</script>

<ul class="tabs">
	<li class="vmap selected"><a href="javascript:vmaptoggle();">Virtual map</a></li>
	<li class="smap"><a href="javascript:smaptoggle();">Street map</a></li>
</ul>
